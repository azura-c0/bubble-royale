{
  "name": "template-vite-ts",
  "description": "A Phaser 3 TypeScript template using Vite.",
  "version": "1.3.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phaserjs/template-vite-ts.git"
  },
  "author": "Phaser Studio <support@phaser.io> (https://phaser.io/)",
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "bugs": {
    "url": "https://github.com/phaserjs/template-vite-ts/issues"
  },
  "homepage": "https://github.com/phaserjs/template-vite-ts#readme",
  "scripts": {
    "dev": "bun log.js dev & vite --config vite/config.dev.mjs",
    "build": "bun log.js build & vite build --config vite/config.prod.mjs",
    "dev-nolog": "bunx --bun vite --config vite/config.dev.mjs",
    "build-nolog": "bunx --bun vite build --config vite/config.prod.mjs",
    "schema": "bunx schema-codegen ../server/src/rooms/schema/GameState.ts --output ./src/schema/ --ts"
  },
  "devDependencies": {
    "terser": "^5.31.0",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "@types/bun": "latest"
  },
  "dependencies": {
    "colyseus.js": "^0.15.26",
    "phaser": "^3.87.0"
  }
}
